## How many types of sql or other words characters in the SQL

-- 1. DDL ( Data Definition Language )
-- 2. DML ( Data Manipulate Language )
-- 3. DCL ( Data Control Language )
-- 4. TCL ( Transaction Control Language )
-- 5. DQL ( Data Query Language ) -> select


-- Create Database and table

create database classroom;

-- if not exists
create database if not exists classroom;
create database if not exists sql_course1;

-- if exits
drop database if exists classroom;

-- create table 
-- step:- 1
use sql_course;
create table if not exists students(
id int primary key,
name varchar(30),
age varchar(30),
gender varchar(30),
address varchar(30),
phone varchar(30),
email_id varchar(30)
);

-- Drop table 
drop table if exists students;


-- step:- 2
create table if not exists my_file.students(
id int primary key,
name varchar(30),
age varchar(30),
gender varchar(30),
address varchar(30),
phone varchar(30),
email_id varchar(30)
);

-- -- fetch the records:-

select * from students;  -- * ( astrick ) all columns 

-- fetch table data form specific database table
select * from my_db.students;

-- specific column read 
select name, address , phone ,email from students;

use sql_course;
select * from sql_course.medical_costs;

select BMI , Children , Smoker, Region,`Medical Cost` from sql_course.medical_costs;
