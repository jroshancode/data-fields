#2025 - 10 - 15 

use classroom;
-- SQL script: create tables and insert 100 rows each for learning JOINs
-- Run in MySQL / MariaDB as-is. For PostgreSQL change AUTO_INCREMENT to SERIAL / IDENTITY.

-- 1) Create train table
CREATE TABLE IF NOT EXISTS train (
    train_id INT PRIMARY KEY,
    train_name VARCHAR(100),
    source VARCHAR(50),
    destination VARCHAR(50),
    capacity INT
);

-- 2) Create passenger table
CREATE TABLE IF NOT EXISTS passenger (
    passenger_id INT PRIMARY KEY,
    passenger_name VARCHAR(100),
    passenger_age INT,
    passenger_gender ENUM('male','female','other'),
    phone VARCHAR(20),
    email VARCHAR(100)
);

-- 3) Create booking table (references train & passenger)
CREATE TABLE IF NOT EXISTS booking (
    booking_id INT PRIMARY KEY,
    train_id INT,
    passenger_id INT,
    travel_date DATE,
    seat_no VARCHAR(10),
    fare DECIMAL(8,2),
    status VARCHAR(20),
    FOREIGN KEY (train_id) REFERENCES train(train_id),
    FOREIGN KEY (passenger_id) REFERENCES passenger(passenger_id)
);

