-- join 

select * from booking;
select * from passenger;
select * from train;


-- left join 

select p.* ,b.* 
from passenger as p 
left join booking as b  
on b.passenger_id = p.passenger_id;


select p.* ,b.* 
from booking as b 
left join passenger as p
on p.passenger_id = b.passenger_id;



-- right join 
select p.* ,b.* 
from passenger as p 
right join booking as b  
on b.passenger_id = p.passenger_id;


select p.* ,b.* 
from booking as b 
right join passenger as p
on p.passenger_id = b.passenger_id;


-- Inner JOin 

select p.* ,b.* 
from passenger as p 
join booking as b  
on b.passenger_id = p.passenger_id;


select p.* ,b.* 
from booking as b 
inner join passenger as p
on p.passenger_id = b.passenger_id;




